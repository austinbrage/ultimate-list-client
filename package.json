{
  "name": "ultimate-list-client",
  "version": "0.0.1",
  "type": "module",
  "main": "root.tsx",
  "author": "Austin Brage",
  "license": "ISC",
  "description": "Client interface for management of users, projects and ideas",
  "keywords": [
    "Management",
    "Projects",
    "Ideas"
  ],
  "scripts": {
    "build": "npm run build:css && remix vite:build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix vite:dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@tanstack/react-query": "^5.24.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/austinbrage/ultimate-list-client"
  },
  "bugs": {
    "url": "https://github.com/austinbrage/ultimate-list-client/issues"
  },
  "homepage": "https://github.com/austinbrage/ultimate-list-client/blob/main/README.md"
}